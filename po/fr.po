#: ../let-me-do.bash:9
msgid "Let Me Do"
msgstr "Laisse Moi Faire"

#: ../let-me-do.bash:12
msgid "You must be logged as root"
msgstr "Vous devez être connecté en tant qu'administrateur"

#: ../let-me-do.bash:25
msgid "Let Me Do is already open"
msgstr "Laisse Moi Faire est déjà ouvert"

#: ../let-me-do.bash:25
msgid "Ok"
msgstr "Ok"

#: ../let-me-do.bash:26
msgid "Open it anyway"
msgstr "L'ouvrir quand même"

#: ../let-me-do.bash:51
msgid "Local IP address not found"
msgstr "L'adresse IP locale n'a pas pu être trouvé"

#: ../let-me-do.bash:59
msgid "# Obtaining the public IP address"
msgstr "# Obtention de l'adresse IP publique"

#: ../let-me-do.bash:62
msgid "Obtaining the public IP address failed"
msgstr "L'obtention de l'adresse IP publique a échoué"

#: ../let-me-do.bash:67
#, sh-format
msgid "# Opening SSH port ($port_ssh) on the router"
msgstr "# Ouverture du port SSH ($port_ssh) sur le routeur"

#: ../let-me-do.bash:69
msgid "Opening SSH port on the router failed"
msgstr "L'ouverture du port SSH sur le routeur a échoué"

#: ../let-me-do.bash:74
#, sh-format
msgid "# Opening VNC port ($port_vnc) on the router"
msgstr "# Ouverture du port VNC ($port_vnc) sur le routeur"

#: ../let-me-do.bash:76
msgid "Opening VNC port on the router failed"
msgstr "L'ouverture du port VNC sur le routeur a échoué"

#: ../let-me-do.bash:81
#, sh-format
msgid "# Redirection of SSH port ($port_ssh to 22)"
msgstr "# Redirection du port SSH ($port_ssh à 22)"

#: ../let-me-do.bash:83
msgid "SSH port redirection failed"
msgstr "La redirection du port SSH a échoué"

#: ../let-me-do.bash:90
msgid "Administrator group not found"
msgstr "Le groupe administrateur n'a pas pu être trouvé"

#: ../let-me-do.bash:94
#, sh-format
msgid "# Adding a new group: $group"
msgstr "# Ajout d'un nouveau groupe : $group"

#: ../let-me-do.bash:96
msgid "The addition of a new group failed:"
msgstr "L'ajout d'un nouveau groupe a échoué :"

#: ../let-me-do.bash:100
msgid "# Adding the user Let Me Do"
msgstr "# Ajout de l'utilisateur Laisse Moi Faire"

#: ../let-me-do.bash:102
msgid "Adding the user Let Me Do failed"
msgstr "L'ajout de l'utilisateur Laisse Moi Faire a échoué"

#: ../let-me-do.bash:106
msgid "Changing Let Me Do's password failed"
msgstr "Le changement du mot de passe de Laisse Moi Faire a échoué"

#: ../let-me-do.bash:110
msgid "# Starting desktop sharing (x11vnc)"
msgstr "# Lancement du partage de bureau (x11vnc)"

#: ../let-me-do.bash:115
msgid "# Copying of information in the clipboard"
msgstr "# Copie des informations dans le presse papier"

#: ../let-me-do.bash:118
msgid "# Wait for reply to if you want to send an email to your master (s)"
msgstr ""
"# Attend la réponse si vous souhaitez envoyer un courrier électronique à "
"votre (vos) maître (s)"

#: ../let-me-do.bash:121
msgid ""
"Would you like to send your connection information by email to your "
"master(s) ?"
msgstr ""
"Voulez-vous envoyer vos informations de connexion par courrier "
"électronique à votre (vos) maître (s)?"

#: ../let-me-do.bash:127
#, sh-format
msgid ""
"# Your computer is now accessible from the Internet.\\nIP address: "
"$internet_ip\\nPassword: $password\\nSSH port: $port_ssh\\nVNC port: "
"$port_vnc\\nThe information required for the connection just be copied to "
"the clipboard."
msgstr ""
"# Votre ordinateur est maintenant accessible depuis Internet.\\nAdresse "
"IP : $internet_ip\\nMot de passe : $password\\nPort SSH : $port_ssh"
"\\nPort VNC : $port_vnc\\nLes informations nécessaires à la connexion "
"viennent d'être copié dans le presse papier."

#: ../let-me-do.bash:130
msgid "Close Let Me Do"
msgstr "Fermer Laisse Moi Faire"

#: ../let-me-do.bash:133
msgid "# Shutting down desktop sharing (x11vnc)"
msgstr "# Arrêt du partage de bureau (x11vnc)"

#: ../let-me-do.bash:136
#, sh-format
msgid "# Closing the SSH port ($port_ssh) on the router"
msgstr "# Fermeture du port SSH ($port_ssh) sur le routeur"

#: ../let-me-do.bash:139
#, sh-format
msgid "# Closing the VNC port ($port_vnc) on the router"
msgstr "# Fermeture du port VNC ($port_vnc) sur le routeur"

#: ../let-me-do.bash:142
#, sh-format
msgid "# Removing SSH port ($port_ssh) redirection"
msgstr "# Supprime la redirection du port SSH ($port_ssh)"

#: ../let-me-do.bash:145
msgid "# Deleting user Let Me Do"
msgstr "# Supprime l'utilisateur Laisse Moi Faire"
